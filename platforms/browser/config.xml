<?xml version='1.0' encoding='utf-8'?>
<widget id="br.gov.rs.cachoeirinha" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Fala Cachoeirinha</name>
    <description>
        Aplicativo para realização de pesquisas realizadas pela Prefeitura Municipal de Cachoeirinha / RS.
    </description>
    <author email="dti@cachoeirinha.rs.gov.br" href="http://www.cachoeirnha.rs.gov.br">
        Fabrica de Software - Prefeitura Municipal de Cachoeirinha - RS
    </author>
    <content src="index.html" />
    <access launch-external="yes" origin="*" />
    <access subdomains="true" uri="*" />
    <acess uri="http://sistemas.cachoeirinha.rs.gov.br/fonts/*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/dialogo.png" />
</widget>
